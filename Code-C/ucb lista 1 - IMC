#include <stdio.h>
int main(){

    float peso, altura, imc;

    printf("Digite seu peso:\n");
    scanf("%f", &peso);

    printf("Digite sua altura\n");
    scanf("%f", &altura);

    imc = peso / (altura * altura); //o calculo do imc Ã© sempre peso dividido por altura ao quadrado

    if(imc < 18.4){
        printf("Voce esta abaixo do peso\n");
    }else if(imc > 18.4 && imc < 24.9){ //se imc foi maior que 18.4 E menor que 24.9.  "&&" = e
        printf("Voce esta no peso normal\n");
    }else if(imc > 24.9 && imc < 29.9){
        printf("Voce esta acima do peso\n");
    }else if(imc > 29.9 && imc < 34.9){
        printf("Voce esta obeso\n");
    }else if(imc < 16.9){
        printf("Voce esta muito abaixo do peso\n");
    }else {
        printf("VOCE E GORDO PRA KRLH KASDKASDKASKDKASDK\n");
    }

    
}
