#include <stdio.h>
int main(){
    int n1,n2,n3;
    int media;

    printf("Digite a primeira nota:\n");
    scanf("%d", &n1);

    printf("Digite a segunda nota:\n");
    scanf("%d", &n2);

    printf("Digite a terceira nota:\n");
    scanf("%d", &n3);

    media = (n1 + n2 + n3) / 3;

    if(media>=6){
        printf("Voce foi aprovado\n");
    }else {
        printf("Voce foi reprovado\n");
    }
}
