#include <stdio.h>

int valor(int delta, int a, int b, int c) {
    delta = (b * b) - (4 * a * c);
}

int main() {
    int delta, a, b, c;
    
    printf("Digite o valor de a: ");
    scanf("%d", &a);
    
    printf("Digite o valor de b: ");
    scanf("%d", &b);
    
    printf("Digite o valor de c: ");
    scanf("%d", &c);
    
    int resultado = valor(delta, a, b, c);
    
    printf("O delta e: %d\n", resultado);
    
}
