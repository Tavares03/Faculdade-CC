#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main(){
    setlocale(LC_ALL,"portuguese");

    int n1 = 0, n2 = 1, i, num, proxTermo;

    printf("Digite quantos números você quer ver da série de Fibonacci\n");
    scanf("%d", &num);

    printf("Sequencia de Fibonacci para %d termos:\n", num);

    for(i=0; i<num; i++){
        printf("%d ", n1);
        proxTermo = n1 + n2;
        n1 = n2;
        n2 = proxTermo;
        
    }



}
